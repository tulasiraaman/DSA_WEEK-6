class Solution {
    public int evalRPN(String[] tokens) {
        Stack<Integer> stack = new Stack<>();
        int num1, num2;

        for(String s: tokens){
            switch(s){
                case "+" -> {
                    num1 = stack.pop();
                    num2 = stack.pop();
                    stack.push(num1 + num2);
                }
                case "*" -> {
                    num1 = stack.pop();
                    num2 = stack.pop();
                    stack.push(num1 * num2);
                }
                case "-" -> {
                    num1 = stack.pop();
                    num2 = stack.pop();
                    stack.push(num2 - num1);
                }
                case "/" -> {
                    num1 = stack.pop();
                    num2 = stack.pop();
                    stack.push(num2 / num1);
                }
                default -> {
                    stack.push(Integer.parseInt(s));
                }
            }
        }
        return stack.pop();
    }
}
